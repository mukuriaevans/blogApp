<%- include("partials/header") -%> 
<h1>Home</h1>
<p><%= homeStartingContent %> </p>

<% posts.forEach(function(post){ %>
    <h4><%= post.title  %> </h4>
    <p><%= post.content.substring(0, 80) + " ..." %> <a href="/posts/<%=post._id%>">Read more</a>   
    </p>
  <% }); %>

  <button class="btn btn-outline-primary"> <a href="/compose">New post</a> </button>

<%- include("partials/footer") -%>